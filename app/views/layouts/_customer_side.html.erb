<div class="container-fluid">
  <div class="row">
    <nav class="col-md-2 d-none d-md-block bg-light sidebar">
      <div class="sidebar-sticky">
        <ul class="nav flex-column">
          <li class="nav-item">
            <%= link_to customer_dashboard_path,class: "nav-link"  do %>
            <i data-feather="home"></i>  Dashboard
            <% end %>
          </li>
          <% if customer_signed_in? %>
          <li class="nav-item">
           <%= link_to customer_customer_path(current_customer.id),class: "nav-link"  do %>
             <i data-feather="users"></i>  Your Profile
           <% end %>
          </li> 
          <li class="nav-item">
            <%= link_to customer_accounts_path,class: "nav-link"  do %>
            <i data-feather="users"></i> My Account
            <% end %>
          </li>
          <li class="nav-item">
            <%= link_to customer_customer_accountants_path,class: "nav-link"  do %>
            <i data-feather="users"></i> My Accountants
            <% end %>
          </li>
          <% end %>
        </ul>
      </div>
    </nav>
  </div>
</div>
<script src="https://unpkg.com/feather-icons/dist/feather.min.js"></script>
<script>
  feather.replace()
</script>